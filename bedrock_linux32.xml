<game_config>
<game_key>bedrock_linux32_linux32</game_key>
<protocol>gameq</protocol>
<gameq_query_name>minecraftpe</gameq_query_name>
<game_name>Minecraft Bedrock</game_name>
<server_exec_name>bedrock_server</server_exec_name>
<mods>
    <mod key="default">
      <name>None</name>
    </mod>
  </mods>
  <replace_texts>
    <text key="ip">
      <default>server-ip=.*</default>
      <var>server-ip=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="port">
      <default>server-port=.*</default>
      <var>server-port=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="true">
      <default>enable-query=.*</default>
      <var>enable-query=true</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="max_players">
      <default>max-players=.*</default>
      <var>max-players=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="server_name">
      <default>server-name=.*</default>
      <var>server-name=</var>
      <filepath>server.properties</filepath>
    </text>
  </replace_texts>
</game_config>